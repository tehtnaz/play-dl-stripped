{
  "name": "play-dl-stripped",
  "version": "1.0.0",
  "description": "YouTube searching",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "build": "tsup",
    "build:check": "tsc --noEmit --incremental false",
    "pretty": "prettier --config .prettierrc \"play-dl/*.ts\" \"play-dl/*/*.ts\" \"play-dl/*/*/*.ts\" --write ",
    "prepublishOnly": "tsup"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tehtnaz/play-dl-stripped"
  },
  "keywords": [
    "node-youtube-dl",
    "youtube-dl",
    "yt-dl",
    "ytdl",
    "youtube"
  ],
  "author": "tehtnaz",
  "license": "GPL-3.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "files": [
    "dist/*"
  ],
  "devDependencies": {
    "@types/node": "^22.17.0",
    "prettier": "^2.3.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  }
}
